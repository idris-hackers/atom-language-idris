name: 'Idris Ipkg'
scopeName: 'source.ipkg'
fileTypes: ['ipkg']
patterns:
  [
    {
      name: 'keyword.control.ipkg'
      match: '\\b(package|opts|modules|sourcedir|makefile|objs|executable|main|libs|pkgs)\\b'
    }
    {
      name: 'string.quoted.double.ipkg'
      begin: '"'
      end: '"'
    }
  ]
